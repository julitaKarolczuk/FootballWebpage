@model SBDProject.Models.Location

@{
    ViewBag.Title = "Szczegóły lokalizacji";
}

<h2>Szczegóły lokalizacji</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Nazwa lokalizacji
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            Adres
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Address)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edytuj", "Edit", new { id = Model.Id }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Powrót do listy lokalizacji", "Index", null, new { @class = "btn btn-primary" })
</p>
